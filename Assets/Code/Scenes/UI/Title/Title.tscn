script/source = "extends Control

func _on_Button_pressed():
	get_tree().change_scene(\"res://Assets/Code/Scenes/Main.tscn\")

func _on_MuteMusic2_pressed():
	$PopUps/PopupPanel.popup_centered()
	$Click.play()

func _on_CloseButton_pressed():
	$PopUps/PopupPanel.visible = false
	$Click.play()

func _on_SoundSlider_value_changed(value):
	AudioServer.set_bus_volume_db(2, linear2db(value / 50))

func _on_MusicSlider_value_changed(value):
	AudioServer.set_bus_volume_db(1, linear2db(value / 50))

func _on_CheckBox2_toggled(button_pressed):  # Mute Music
	AudioServer.set_bus_mute(1, button_pressed)
	$Click.play()

func _on_CheckBox_toggled(button_pressed):  # Mute Sound
	AudioServer.set_bus_mute(2, button_pressed)
	$Click.play()
"

